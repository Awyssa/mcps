  "packageManager": "pnpm@10.6.3",
  "dependencies": {
    "concurrently": "^9.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "prettier": "^3.7.4"
  }
}
